<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@drawable/bg1"
    android:layout_width="match_parent"
    android:padding="@dimen/_16sdp"
    android:layout_height="match_parent"
    tools:context=".Enfp">

    <!-- Header Section -->
    <FrameLayout
        android:layout_marginTop="@dimen/_16sdp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_34sdp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="ENFP"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:textSize="@dimen/_20ssp" />

        <TextView
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="The Campaigner"
            android:textColor="@color/white"
            android:textSize="@dimen/_10ssp" />

    </FrameLayout>

    <!-- Traits Section -->
    <androidx.cardview.widget.CardView
        android:layout_marginTop="@dimen/_6sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/lightgreen"
        app:cardCornerRadius="@dimen/_8sdp">

        <LinearLayout
            android:paddingVertical="@dimen/_6sdp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Traits"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Enthusiastic: ENFPs are highly energetic and enthusiastic."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Creative: They are imaginative and often come up with innovative ideas."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Empathetic: ENFPs are deeply understanding and empathetic towards others."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Spontaneous: They enjoy living in the moment and embracing new experiences."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <!-- Relationships Section -->
    <androidx.cardview.widget.CardView
        android:layout_marginTop="@dimen/_6sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/darkgreen"
        app:cardCornerRadius="@dimen/_8sdp">

        <LinearLayout
            android:paddingVertical="@dimen/_6sdp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="In Relationships"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Passionate: ENFPs bring a lot of passion and excitement into their relationships."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Supportive: They are incredibly supportive and understanding partners."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="• Adventurous: ENFPs love to explore new activities and experiences with their partner."
                android:textColor="@color/white"
                android:fontFamily="@font/poppins"
                android:paddingHorizontal="@dimen/_6sdp"/>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <!-- Challenges and Strengths Section -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <!-- Challenges Section -->
        <androidx.cardview.widget.CardView
            android:layout_weight="1"
            android:layout_marginEnd="@dimen/_3sdp"
            android:layout_marginTop="@dimen/_6sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/darkgreen"
            app:cardCornerRadius="@dimen/_8sdp">

            <LinearLayout
                android:paddingVertical="@dimen/_6sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Challenges"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Disorganized"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Overly Idealistic"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Easily Distracted"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <!-- Strengths Section -->
        <androidx.cardview.widget.CardView
            android:layout_weight="1"
            android:layout_marginStart="@dimen/_3sdp"
            android:layout_marginTop="@dimen/_6sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/darkgreen"
            app:cardCornerRadius="@dimen/_8sdp">

            <LinearLayout
                android:paddingVertical="@dimen/_6sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Strengths"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Charismatic and Inspiring"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Empathetic and Understanding"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="• Energetic and Passionate"
                    android:textColor="@color/white"
                    android:fontFamily="@font/poppins"
                    android:paddingHorizontal="@dimen/_6sdp"/>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

    </LinearLayout>
    >

</LinearLayout>
